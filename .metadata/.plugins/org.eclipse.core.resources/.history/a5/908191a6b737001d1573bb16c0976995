package world;

import java.util.Scanner;

public class Helloworld {

public static void main(String[] args) {
	Scanner in = new Scanner(System.in);
    
    int A=in.nextInt();
    int B=in.nextInt();
    
    in.close();
    
    System.out.prinln(A+B);
    System.out.prinln(A-B);
    System.out.prinln(A*B);
    System.out.prinln(A/B);
    System.out.prinln(A%B);
			}

}
